{
  "private": true,
  "name": "npdu",
  "version": "0.0.0",
  "description": "Update your Node.js package dependencies (SemVer compliant)",
  "license": "MIT",
  "author": {
    "name": "Jimi (Dimitris) Charalampidis",
    "email": "jimikar@gmail.com"
  },
  "repository": "JimiC/npdu",
  "engines": {
    "node": ">=6.5.0"
  },
  "keywords": [
    "dep",
    "dependency",
    "dependencies",
    "node",
    "nodejs",
    "node.js",
    "npm",
    "packages",
    "package.json",
    "semver",
    "update",
    "updater"
  ],
  "main": "lib/npdu.js",
  "bin": {
    "npdu": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "types": "lib/npdu.d.ts",
  "scripts": {
    "start": "node ./out/src/cli.js",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "npm run test",
    "precompile:dev": "npm run precompile",
    "compile:dev": "tsc -p tsconfig.dev.json",
    "compile:w": "npm run compile:dev -- -w",
    "prebuild": "npm run lint",
    "build": "npm run compile",
    "precompile": "rimraf ./out",
    "compile": "tsc",
    "lint": "tslint -p .",
    "pretest": "rimraf ./.nyc_output ./coverage && npm run compile:dev",
    "test": "nyc -c -x '' mocha ./out/test",
    "posttest": "nyc report -r lcov",
    "preversion": "npm t",
    "prepublishOnly": "rimraf ./lib && npm run build && cpx 'out/src/**/*' lib"
  },
  "dependencies": {
    "detect-indent": "^5.0.0",
    "semver": "^5.5.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cpx": "^1.5.0",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "@types/chai": "^4.1.2",
    "@types/detect-indent": "^5.0.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^6",
    "@types/semver": "^5.5.0",
    "@types/sinon": "^4.1.3",
    "@types/yargs": "^11.0.0"
  }
}
