{
    "name": "npdu",
    "version": "0.0.0",
    "description": "Update your Node.js package dependencies (SemVer compliant)",
    "license": "MIT",
    "author": {
        "name": "Jimi (Dimitris) Charalampidis",
        "email": "jimikar@gmail.com"
    },
    "repository": "JimiC/node-pack-dep-upd",
    "engines": {
        "node": ">=6.5.0"
    },
    "keywords": [
        "dep",
        "dependency",
        "dependencies",
        "node",
        "nodejs",
        "node.js",
        "npm",
        "packages",
        "package.json",
        "semver",
        "update",
        "updater"
    ],
    "main": "lib/npdu.js",
    "bin": {
        "npdu": "lib/cli.js"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "start": "node ./lib/cli.js",
        "build": "npm run lint && npm run test",
        "precompile": "rimraf ./out",
        "compile": "tsc",
        "compile:w": "npm run compile -- -w",
        "lint": "tslint -p .",
        "pretest": "rimraf ./.nyc_output ./coverage && npm run compile",
        "test": "nyc -c -x '' mocha ./out/test --recursive",
        "posttest": "nyc report -r lcov",
        "prepublishOnly": "rimraf ./lib && npm run build && ncp out/src lib"
    },
    "dependencies": {
        "semver": "^5.5.0",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "codecov": "^3.0.0",
        "ncp": "^2.0.0",
        "mocha": "^5.0.1",
        "nyc": "^11.4.1",
        "rimraf": "^2.6.2",
        "tslint": "^5.9.1",
        "typescript": "^2.7.2",
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^6",
        "@types/semver": "^5.5.0",
        "@types/yargs": "^11.0.0"
    }
}
